{
    "compilerOptions": {
      "target": "ES2021",
      "module": "ESNext",
      "declaration": true,
      "declarationMap": true,
      "outDir": "dist",
      "strict": true,
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true
    },
    "include": ["**/*.ts"]
  }
  